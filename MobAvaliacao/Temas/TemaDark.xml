<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobAvaliacao.TemaDark">

	<Color x:Key="PageBackgroundColor">Black</Color>
	<Color x:Key="NavigationBarColor">Green</Color>
	<Color x:Key="ShellBarColor">Black</Color>

	<Color x:Key="UnselectedColor">#909090</Color>
	<Color x:Key="TabUnselectedColor">#003B00</Color>
	<Color x:Key="DisabledColor">Gray</Color>
	<Color x:Key="DisabledColorButton">#FF6347</Color>

	<Color x:Key="PrimaryColor">White</Color>
	<Color x:Key="SecondaryColor">White</Color>
	<Color x:Key="TertiaryColor">Green</Color>
	<Color x:Key="QuartenaryColor">Red</Color>

	<Color x:Key="PrimaryTextColor">White</Color>
	<Color x:Key="SecondaryTextColor">White</Color>
	<Color x:Key="TertiaryTextColor">Black</Color>

	<Color x:Key="TransparentColor">Transparent</Color>

	<Style x:Key="ModoApp"
           TargetType="Image">
		<Setter Property="Source"
                Value="modolight"/>
	</Style>
</ResourceDictionary>